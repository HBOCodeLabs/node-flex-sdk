    "url": "git://github.com/JamesMGreene/node-mxmlc-install.git"
  },
  "bugs": {
    "url": "https://github.com/JamesMGreene/node-mxmlc-install/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/JamesMGreene/node-mxmlc-install/blob/master/LICENSE-MIT"
    }
